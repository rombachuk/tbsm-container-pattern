array myapps;

if( match( @Manager, "ProbeWatch" ) )
{
        switch(@Summary)
        {
        case "Running ...":
                @Severity = 1
                @AlertGroup = "probestat"
                @Type = 2
        case "Going Down ...":
                @Severity = 5
                @AlertGroup = "probestat"
                @Type = 1
        default:
                @Severity = 1
        }
        @AlertKey = @Agent
        @Summary = @Agent + " probe on " + @Node + ": " + @Summary
}
else
{
	@Manager	= "Simnet Probe"
	@Class		= 3300
	@Node 		= $Node
	@Agent 		= $Agent
	@AlertGroup 	= $Group
	@Summary 	= $Summary
	@Severity 	= $Severity
        @BSM_Identity   = "Simulated Probe Events"

        # Assign AlertGroup to Application Name
        $num_fields = split($Node,myapps,"-")
        @AlertGroup = myapps[1]
        @AlertKey = myapps[2]
      
        if ( regmatch($Node, ".*rtr001.*")) {
          @Location = "Charlotte"
        }

        if (regmatch($Node, ".*esx01.*") 
           || regmatch($Node, ".*esx02.*") 
          ){
          @Location = "Dallas"
        }

        if ( regmatch($Node, ".*rtr01.*") 
           || regmatch($Node, ".*rtr02.*") 
           || regmatch($Node, ".*rtr201.*") 
        ){
          @Location = "Dallas"
        }

        if ( regmatch($Node, ".*app01.*")) {
          @Location = "Dallas"
        }

        if ( regmatch($Node, ".*app02.*")) {
          @Location = "Dallas"
        }

        if (regmatch($Node, ".*esx03.*") 
           || regmatch($Node, ".*esx04.*") 
           || regmatch($Node, ".*esx05.*") 
        ){
          @Location = "Chicago"
        }

        if (regmatch($Node, ".*rtr03.*") 
           || regmatch($Node, ".*rtr04.*") 
           || regmatch($Node, ".*rtr05.*") 
        ){
          @Location = "Chicago"
        }

        if ( regmatch($Node, ".*app03.*")) {
          @Location = "Chicago"
        }

        if ( regmatch($Node, ".*app04.*")) {
          @Location = "Chicago"
        }

        if ( regmatch($Node, ".*app05.*")) {
          @Location = "Chicago"
        }

        if (regmatch($Node, ".*esx06.*") 
        ){
          @Location = "Seattle"
        }
        if (regmatch($Node, ".*rtr06.*") 
        ){
          @Location = "Seattle"
        }

        if ( regmatch($Node, ".*app06.*")) {
          @Location = "Seattle"
        }

        if (regmatch($Node, ".*esx07.*") 
           || regmatch($Node, ".*esx08.*") 
           || regmatch($Node, ".*esx09.*") 
        ){
          @Location = "LosAngeles"
        }

        if (regmatch($Node, ".*rtr07.*") 
           || regmatch($Node, ".*rtr08.*") 
           || regmatch($Node, ".*rtr09.*") 
        ){
          @Location = "LosAngeles"
        }

        if ( regmatch($Node, ".*app07.*")) {
          @Location = "LosAngeles"
        }

        if ( regmatch($Node, ".*app08.*")) {
          @Location = "LosAngeles"
        }

        if ( regmatch($Node, ".*app09.*")) {
          @Location = "LosAngeles"
        }

        if (regmatch($Node, ".*esx10.*") 
           || regmatch($Node, ".*esx11.*") 
        ){
          @Location = "Cleveland"
        }

        if (regmatch($Node, ".*rtr10.*") 
           || regmatch($Node, ".*rtr11.*") 
        ){
          @Location = "Cleveland"
        }

        if ( regmatch($Node, ".*app10.*")) {
          @Location = "Cleveland"
        }

        if ( regmatch($Node, ".*app11.*")) {
          @Location = "Cleveland"
        }

        if (regmatch($Node, ".*esx12.*") 
           || regmatch($Node, ".*esx13.*") 
           || regmatch($Node, ".*esx14.*") 
        ){
          @Location = "KansasCity"
        }

        if (regmatch($Node, ".*rtr12.*") 
           || regmatch($Node, ".*rtr13.*") 
           || regmatch($Node, ".*rtr14.*") 
        ){
          @Location = "KansasCity"
        }

        if ( regmatch($Node, ".*app12.*")) {
          @Location = "KansasCity"
        }

        if ( regmatch($Node, ".*app13.*")) {
          @Location = "KansasCity"
        }

        if ( regmatch($Node, ".*app14.*")) {
          @Location = "KansasCity"
        }

        if (regmatch($Node, ".*esx15.*") 
           || regmatch($Node, ".*esx16.*") 
           || regmatch($Node, ".*esx17.*") 
        ){
          @Location = "Boston"
        }

        if (regmatch($Node, ".*rtr15.*") 
           || regmatch($Node, ".*rtr16.*") 
           || regmatch($Node, ".*rtr17.*") 
        ){
          @Location = "Boston"
        }

        if ( regmatch($Node, ".*app15.*")) {
          @Location = "Boston"
        }

        if ( regmatch($Node, ".*app16.*")) {
          @Location = "Boston"
        }

        if ( regmatch($Node, ".*app17.*")) {
          @Location = "Boston"
        }

        if (regmatch($Node, ".*esx18.*") 
        ){
          @Location = "Raleigh"
        }
        if (regmatch($Node, ".*rtr18.*") 
        ){
          @Location = "Raleigh"
        }

        if ( regmatch($Node, ".*app18.*")) {
          @Location = "Raleigh"
        }

        if (regmatch($Node, ".*esx19.*") 
           || regmatch($Node, ".*esx20.*") 
        ){
          @Location = "Atlanta"
        }
        if (regmatch($Node, ".*rtr19.*") 
           || regmatch($Node, ".*rtr20.*") 
        ){
          @Location = "Atlanta"
        }

        if ( regmatch($Node, ".*app19.*")) {
          @Location = "Atlanta"
        }

        if ( regmatch($Node, ".*app20.*")) {
          @Location = "Atlanta"
        }

        if ( regmatch($Node, ".*fw.*")) {
          @Location = myapps[1]
        }

	if (nmatch($Summary, "Port failure"))
	{
		#@AlertKey = $PortNumber
               switch ($PortNumber) {
                   case "1":
                     @Summary = "JVM CPU Utilitzation High 75%"
                     @Service = "App"
                     @Severity = 3
                   case "2":
		     @Summary = "Login Transaction Response time high 5secs"
                     @Service = "Trans"
                     @Severity = 4
                   case "3":
                     @Summary = "A Web Site received too many requests per second."
                     @Service = "Web"
                     @Severity = 3 
                   case "4":
                     @Summary = "DB2 CPU Utilisation High 75 %"
                     @Service = "DB"
                     @Severity = 3 
                   default:
                     @Summary = "Critical Application Error"
                     @Severity = 5
                     @Service = "App"
               }
	}
	else if (nmatch($Summary, "Diskspace"))
	{
	#	@AlertKey = $PercentFull + "% full"
                @Service = "OS"
                @Summary = "Diskspace Alert "+$PercentFull + "% full"
	}

	if(regmatch($Summary, ".*Down.*")){
		@Identifier = "Down" + @Identifier 
		@Type = 1
                @Severity = 5
                @Service = "Network"
	}
	if(regmatch($Summary, ".*Up.*")){
		@Type = 2
                @Severity =  1             
                @Service = "Network"
	}

	if(regmatch($Summary, ".*offline.*")){
		@Identifier = "offline" + @Identifier 
		@Type = 1
                @Service = "VS"
                @Severity = 5
                @Summary = "A Virtual Machine has gone offline."
	}
	if(regmatch($Summary, ".*online.*")){
		@Type = 2
                @Service = "VS"
                @Severity = 1        
                @Summary = "A Virtual Machine has come online."
	}
	@Identifier 	= $Node + $Agent + $Severity + $AlertGroup + $AlertKey
}

